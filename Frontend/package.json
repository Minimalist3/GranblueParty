{
  "name": "gbf-frontend",
  "version": "2.1.0",
  "description": "Granblue Party Frontend",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "build:static": "cross-env NODE_ENV=production webpack --config webpack.static.config.js",
    "build": "npm run build:server && npm run build:client && npm run build:static",
    "dev:server": "webpack -w --config webpack.server.config.js",
    "dev:client": "webpack server --config webpack.client.config.js",
    "dev:static": "webpack --config webpack.static.config.js",
    "nodemon": "nodemon --trace-warnings server.js"
  },
  "author": {
    "name": "Minimalist",
    "email": "contact@granblue.party",
    "url": "https://www.granblue.party"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.13",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "cssnano": "^5.1.14",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "html-webpack-plugin": "^5.5.0",
    "ifdef-loader": "^2.3.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "sitemap-webpack-plugin": "^1.1.1",
    "url": "^0.11.0",
    "vue-loader": "^15.10.1",
    "webpack": "^5.75.0",
    "webpack-cdn-plugin": "^3.3.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^2.0.9",
    "axios": "^1.1.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "tailwindcss": "^3.2.4",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vue-server-renderer": "^2.7.14",
    "vuex": "^3.6.2"
  },
  "packageManager": "yarn@3.2.3"
}
